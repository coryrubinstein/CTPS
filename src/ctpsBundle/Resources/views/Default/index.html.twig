{% extends 'base.html.twig' %}


{% block stylesheets %}

<style>

    body {
        background-color: beige;
    }

    .center {
        text-align: center;
    }

    h1 {
        text-decoration: underline;
    }

    #landingButton {
        float: left;
    }

    #nextButton {
        float: right;
    }




</style>

{% endblock %}


{% block body %}



    <h1 CLASS="center">Welcome To My Page</h1>

    <br>
    <br>

    <form action="http://127.0.0.1:8000/landing" target="_blank">
        <input id="landingButton" type="submit" value="Landing Page"  />
    </form>

    <form action="http://127.0.0.1:8000/landing/next" target="_blank">
        <input id="nextButton" type="submit" value="Next Page"  />
    </form>



{% endblock %}